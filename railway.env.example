APP_NAME="Invenza"
APP_ENV=production
APP_DEBUG=false
APP_KEY=base64:your_app_key_here

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=single
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error
LOG_SLACK_WEBHOOK_URL=null
LOG_STDERR_FORMATTER=null
PAPERTRAIL_URL=logs.papertrailapp.com
PAPERTRAIL_PORT=12345
SLACK_BOT_USER_DEFAULT_CHANNEL=null
SLACK_BOT_USER_OAUTH_TOKEN=null
DYNAMODB_ENDPOINT=null

# Railway auto-injects MySQL variables
DB_CONNECTION=mysql
DB_HOST=${MYSQL_HOST}
DB_PORT=${MYSQL_PORT}
DB_DATABASE=${MYSQL_DATABASE}
DB_USERNAME=${MYSQL_USER}
DB_PASSWORD=${MYSQL_PASSWORD}
DB_URL=null
MYSQL_ATTR_SSL_CA=null

# Session Configuration
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null
SESSION_CONNECTION=null
SESSION_STORE=null
SESSION_SECURE_COOKIE=true

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# Queue Configuration  
QUEUE_CONNECTION=database
DB_QUEUE_CONNECTION=null
SQS_SUFFIX=null

# Cache Configuration
CACHE_STORE=database
CACHE_PREFIX=
DB_CACHE_CONNECTION=null
DB_CACHE_LOCK_CONNECTION=null
DB_CACHE_LOCK_TABLE=null

# Memcached
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PERSISTENT_ID=null
MEMCACHED_USERNAME=null
MEMCACHED_PASSWORD=null

# Redis
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_URL=null
REDIS_USERNAME=null

# Mail Configuration
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="noreply@invenza.com"
MAIL_FROM_NAME="${APP_NAME}"
MAIL_URL=null
MAIL_LOG_CHANNEL=null
POSTMARK_MESSAGE_STREAM_ID=null
POSTMARK_TOKEN=null
RESEND_KEY=null

# AWS (not used)
AWS_ACCESS_KEY_ID=null
AWS_SECRET_ACCESS_KEY=null
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=null
AWS_USE_PATH_STYLE_ENDPOINT=false
AWS_URL=null
AWS_ENDPOINT=null

# Vite
VITE_APP_NAME="${APP_NAME}"

# Stripe - Add your actual keys in Railway dashboard
STRIPE_SECRET=sk_test_your_test_key_here
STRIPE_KEY=pk_test_your_test_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here